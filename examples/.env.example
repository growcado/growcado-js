# Growcado SDK Examples - Environment Configuration
# Copy this file to .env to set default values for all examples

# SDK Configuration
VITE_DEFAULT_TENANT_ID=demo-tenant
VITE_DEFAULT_BASE_URL=https://api.growcado.io/
VITE_DEFAULT_STORAGE=localStorage
VITE_DEFAULT_ENABLE_AUTO_UTM=true
VITE_DEFAULT_ENABLE_REFERRER_TRACKING=true

# Customer Identifiers
VITE_DEFAULT_EMAIL=user@example.com
VITE_DEFAULT_USER_ID=user123
VITE_DEFAULT_ANONYMOUS_ID=anon-abc123

# Content Configuration
VITE_DEFAULT_MODEL_IDENTIFIER=homepage
VITE_DEFAULT_CONTENT_IDENTIFIER=hero-section

# Custom Headers (JSON format)
VITE_DEFAULT_CUSTOM_HEADERS={"Authorization": "Bearer token"}

# Additional Custom Fields
VITE_DEFAULT_CUSTOM_KEY=customField
VITE_DEFAULT_CUSTOM_VALUE=customValue
