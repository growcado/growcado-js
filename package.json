{
  "name": "@growcado/growcado-js",
  "version": "1.0.0",
  "description": "TypeScript SDK for Growcado",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "nx run-many -t build",
    "build:packages": "nx run-many -t build --projects=sdk,react",
    "test": "nx run-many -t test",
    "lint": "nx run-many -t lint",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "typecheck": "nx run-many -t typecheck",
    "pre-push": "pnpm run pre-push:check",
    "pre-push:check": "pnpm run typecheck && pnpm run lint && npx nx run sdk:test && npx nx run sdk:build",
    "pre-push:fix": "pnpm run typecheck && pnpm run lint && pnpm run test && pnpm run build",
    "pre-push:script": "./scripts/pre-push.sh",
    "verify": "pnpm run pre-push:check",
    "publish:sdk:patch": "./scripts/publish-package.sh sdk patch",
    "publish:react:patch": "./scripts/publish-package.sh react patch",
    "publish:all:patch": "./scripts/publish-package.sh sdk patch && ./scripts/publish-package.sh react patch",
    "publish:sdk:minor": "./scripts/publish-package.sh sdk minor",
    "publish:react:minor": "./scripts/publish-package.sh react minor",
    "publish:sdk:major": "./scripts/publish-package.sh sdk major",
    "publish:react:major": "./scripts/publish-package.sh react major",
    "publish:dry-run:sdk": "./scripts/publish-package-dry-run.sh sdk patch",
    "publish:dry-run:react": "./scripts/publish-package-dry-run.sh react patch",
    "publish:help": "./scripts/show-publish-help.sh",
    "example:vanilla": "cd examples/vanilla-sdk && pnpm install --ignore-scripts && pnpm run dev",
    "example:react": "cd examples/react-app && pnpm install --ignore-scripts && pnpm run dev"
  },
  "keywords": [
    "growcado",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "21.2.1",
    "@nx/eslint-plugin": "21.2.1",
    "@nx/js": "^21.2.1",
    "@nx/react": "^21.2.1",
    "@nx/rollup": "21.2.1",
    "@nx/vite": "21.2.1",
    "@nx/web": "21.2.1",
    "@nx/workspace": "^21.2.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/rollup": "^8.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tanstack/react-query": "^5.0.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "core-js": "^3.36.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "^21.2.1",
    "prettier": "^2.6.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rollup": "^4.14.0",
    "swc-loader": "0.1.15",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "packages/*"
  ],
  "nx": {
    "includedScripts": []
  }
}
