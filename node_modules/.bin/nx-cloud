#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/nx@21.2.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.23_ty_j2hzvjgup4zohk4cuk7eczbr2a/node_modules/nx/bin/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/nx@21.2.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.23_ty_j2hzvjgup4zohk4cuk7eczbr2a/node_modules/nx/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/nx@21.2.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.23_ty_j2hzvjgup4zohk4cuk7eczbr2a/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/nx@21.2.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.23_ty_j2hzvjgup4zohk4cuk7eczbr2a/node_modules/nx/bin/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/nx@21.2.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.23_ty_j2hzvjgup4zohk4cuk7eczbr2a/node_modules/nx/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/nx@21.2.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.23_ty_j2hzvjgup4zohk4cuk7eczbr2a/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nx/bin/nx-cloud.js" "$@"
else
  exec node  "$basedir/../nx/bin/nx-cloud.js" "$@"
fi
