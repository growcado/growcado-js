#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/verdaccio@6.1.4_encoding@0.1.13_typanion@3.14.0/node_modules/verdaccio/bin/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/verdaccio@6.1.4_encoding@0.1.13_typanion@3.14.0/node_modules/verdaccio/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/verdaccio@6.1.4_encoding@0.1.13_typanion@3.14.0/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/verdaccio@6.1.4_encoding@0.1.13_typanion@3.14.0/node_modules/verdaccio/bin/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/verdaccio@6.1.4_encoding@0.1.13_typanion@3.14.0/node_modules/verdaccio/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/verdaccio@6.1.4_encoding@0.1.13_typanion@3.14.0/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../verdaccio/bin/verdaccio" "$@"
else
  exec node  "$basedir/../verdaccio/bin/verdaccio" "$@"
fi
