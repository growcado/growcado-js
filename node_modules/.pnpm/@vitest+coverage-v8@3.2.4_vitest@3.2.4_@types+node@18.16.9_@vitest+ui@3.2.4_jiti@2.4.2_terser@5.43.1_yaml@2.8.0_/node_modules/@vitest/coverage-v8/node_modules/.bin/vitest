#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/vitest@3.2.4_@types+node@18.16.9_@vitest+ui@3.2.4_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vitest/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/vitest@3.2.4_@types+node@18.16.9_@vitest+ui@3.2.4_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/vitest@3.2.4_@types+node@18.16.9_@vitest+ui@3.2.4_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vitest/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/vitest@3.2.4_@types+node@18.16.9_@vitest+ui@3.2.4_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules:/Users/masoudaghaei/Growcado/codes/growcado-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@3.2.4_@types+node@18.16.9_@vitest+ui@3.2.4_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@3.2.4_@types+node@18.16.9_@vitest+ui@3.2.4_jiti@2.4.2_terser@5.43.1_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
fi
