#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/var/folders/tj/spcbh8lj31l7w4w_kwj0f3200000gn/T/tmp-74625-R3MhOmdGi4cC/node_modules/.pnpm/yaml@2.8.0/node_modules/yaml/node_modules:/private/var/folders/tj/spcbh8lj31l7w4w_kwj0f3200000gn/T/tmp-74625-R3MhOmdGi4cC/node_modules/.pnpm/yaml@2.8.0/node_modules:/private/var/folders/tj/spcbh8lj31l7w4w_kwj0f3200000gn/T/tmp-74625-R3MhOmdGi4cC/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/private/var/folders/tj/spcbh8lj31l7w4w_kwj0f3200000gn/T/tmp-74625-R3MhOmdGi4cC/node_modules/.pnpm/yaml@2.8.0/node_modules/yaml/node_modules:/private/var/folders/tj/spcbh8lj31l7w4w_kwj0f3200000gn/T/tmp-74625-R3MhOmdGi4cC/node_modules/.pnpm/yaml@2.8.0/node_modules:/private/var/folders/tj/spcbh8lj31l7w4w_kwj0f3200000gn/T/tmp-74625-R3MhOmdGi4cC/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../yaml@2.8.0/node_modules/yaml/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../yaml@2.8.0/node_modules/yaml/bin.mjs" "$@"
fi
