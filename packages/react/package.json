{
  "name": "@growcado/react",
  "version": "0.0.1",
  "description": "React hooks and components for Growcado SDK",
  "main": "./index.js",
  "module": "./index.esm.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.js"
    }
  },
  "files": [
    "*.d.ts",
    "*.js",
    "*.js.map"
  ],
  "dependencies": {
    "@growcado/sdk": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@tanstack/react-query": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "keywords": [
    "growcado",
    "react",
    "hooks",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/growcado/growcado-js",
    "directory": "packages/react"
  }
}
